{
  "8948203864933858215": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/sample",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/sample/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/sample/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/sample"
      }
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/sample"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/sample"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "npx nx watch --projects @voices/voices --includeDependentProjects -- npx nx build-deps @voices/voices"
    }
  },
